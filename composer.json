{
  "require": {
    "barrelstrength/sprout-forms": "3.13.5",
    "barrelstrength/sprout-forms-google-recaptcha": "1.4.0",
    "craftcms/aws-s3": "1.2.11",
    "craftcms/cms": "3.6.12",
    "craftcms/redactor": "2.8.6",
    "dolphiq/sitemap": "1.3.2",
    "sebastianlenz/linkfield": "1.0.25",
    "topshelfcraft/wordsmith": "3.3.0.1",
    "verbb/navigation": "1.4.15",
    "verbb/super-table": "2.6.7",
    "vlucas/phpdotenv": "^3.4.0"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform": {
      "php": "7.2.5"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ]
  }
}
