{
  "require": {
    "balazscsaba2006/cookiebot": "1.0.9",
    "barrelstrength/sprout-forms": "3.12.2",
    "craftcms/aws-s3": "1.2.9",
    "craftcms/cms": "3.5.0",
    "craftcms/redactor": "2.6.1",
    "dolphiq/sitemap": "1.3.0",
    "sebastianlenz/linkfield": "1.0.23",
    "verbb/field-manager": "2.1.8",
    "verbb/navigation": "1.3.23",
    "verbb/super-table": "2.5.2",
    "vlucas/phpdotenv": "^3.4.0"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform": {
      "php": "7.0"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ]
  }
}
