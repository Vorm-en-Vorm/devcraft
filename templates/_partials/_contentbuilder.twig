<div class="content-blocks">
{% set blocks = entry.contentbuilder.all() %}

	{% for block in blocks %}
	
		{% if block.type == "afbeeldingTekst" %}
			{% include '_partials/contentblokken/afbeelding-tekst.html' %}

		{% elseif block.type == "mediaVolleBreedte" %}
			{% include '_partials/contentblokken/fullwidth-media.html' %}
		
		{% elseif block.type == "textVolleBreedte" %}
			{% include '_partials/contentblokken/fullwidth-text.html' %}

        {% endif %}

    {% endfor %}
</div>